# host github.com
#     Hostname ssh.github.com
#     Port 443
# git@github.com

# 多账户配置
# 详细参见 https://git.deepjs.net/git-config.html#git-%E5%A4%9A%E8%B4%A6%E5%8F%B7%E9%85%8D%E7%BD%AE%E9%97%AE%E9%A2%98

host github.com
    Hostname github.com
    # User xx2
    IdentityFile ~/.ssh/xiaohan_rsa

# 公司的 gitlab
host gitlab.iqianggou.com
    Hostname gitlab.iqianggou.com
    User git
    IdentityFile ~/.ssh/xiaohan

host gitee.com
    Hostname gitee.com
    User git
    IdentityFile ~/.ssh/gitee_rsa

# git@gitlab.xxx.com
